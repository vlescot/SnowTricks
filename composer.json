{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "doctrine/doctrine-migrations-bundle": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.12",
        "ramsey/uuid": "^3.7",
        "ramsey/uuid-doctrine": "^1.5",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/asset": "^4.0",
        "symfony/console": "^4.0",
        "symfony/dom-crawler": "^4.0",
        "symfony/filesystem": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.3",
        "symfony/orm-pack": "^1.0",
        "symfony/panther": "^0.1.0",
        "symfony/security-bundle": "^4.0",
        "symfony/security-csrf": "^4.0",
        "symfony/security-guard": "^4.0",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/validator": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "escapestudios/symfony2-coding-standard": "3.x-dev",
        "phpspec/prophecy": "~1.0",
        "phpunit/php-code-coverage": "^6.0",
        "symfony/browser-kit": "^4.0",
        "symfony/css-selector": "^4.0",
        "symfony/debug": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/var-dumper": "^4.0",
        "symfony/web-server-bundle": "^4.0"
    },
    "config": {
        "optimize-autoloader": true,
        "apcu-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Tests\\": "tests/",
            "App\\App\\": "src/App/Service/",
            "App\\App\\Interfaces\\": "src/App/Service/Interfaces/",
            "App\\App\\Image\\": "src/App/Service/Image/",
            "App\\App\\Image\\Interfaces\\": "src/App/Service/Image/Interfaces/",
            "App\\App\\Security\\": "src/UI/Security/",
            "App\\App\\Security\\Interfaces\\": "src/UI/Security/Interfaces/",
            "App\\Domain\\Builder\\": "src/Domain/Builder/",
            "App\\Domain\\Builder\\Interfaces\\": "src/Domain/Builder/Interfaces/",
            "App\\Domain\\CollectionManager\\": "src/App/Service/CollectionManager/",
            "App\\Domain\\CollectionManager\\Interfaces\\": "src/App/Service/CollectionManager/Interfaces/",
            "App\\Domain\\DTO\\": "src/Domain/DTO/",
            "App\\Domain\\DTO\\Interfaces\\": "src/Domain/DTO/Interfaces/",
            "App\\Domain\\Entity\\": "src/Domain/Entity/",
            "App\\Domain\\Entity\\Interfaces\\": "src/Domain/Entity/Interfaces/",
            "App\\Domain\\Factory\\": "src/Domain/Factory/",
            "App\\Domain\\Factory\\Interfaces\\": "src/Domain/Factory/Interfaces/",
            "App\\Domain\\Repository\\": "src/Domain/Repository/",
            "App\\Domain\\Repository\\Interfaces\\": "src/Domain/Repository/Interfaces/",
            "App\\UI\\Action\\": "src/UI/Action/",
            "App\\UI\\Action\\Interfaces\\": "src/UI/Action/Interfaces/",
            "App\\UI\\Action\\Authentication\\": "src/UI/Action/Authentication/",
            "App\\UI\\Action\\Authentication\\Interfaces\\": "src/UI/Action/Interfaces/Authentication/",
            "App\\UI\\Form\\": "src/UI/Form/",
            "App\\UI\\Form\\Interfaces\\": "src/UI/Form/Interfaces/",
            "App\\UI\\Form\\Handler\\": "src/UI/Form/Handler/",
            "App\\UI\\Form\\Handler\\Interfaces\\": "src/UI/Form/Interfaces/Handler/",
            "App\\UI\\Form\\Subscriber\\": "src/UI/Form/Subscriber/",
            "App\\UI\\Form\\Subscriber\\Interfaces\\": "src/UI/Form/Interfaces/Subscriber/",
            "App\\UI\\Form\\Type\\": "src/UI/Form/Type/",
            "App\\UI\\Form\\Type\\Interfaces\\": "src/UI/Form/Interfaces/Type/",
            "App\\UI\\Form\\Type\\Authentication\\": "src/UI/Form/Interfaces/Type/Authentication/",
            "App\\UI\\Form\\Type\\Interfaces\\Authentication\\": "src/UI/Form/Interfaces/Type/Authentication/",
            "App\\UI\\Responder\\": "src/UI/Responder/",
            "App\\UI\\Responder\\Interfaces\\": "src/UI/Responder/Interfaces/"
        }
    },
  "autoload-dev": {
      "psr-4": {
          "App\\Tests\\": "tests/",
          "App\\": "src/",
          "App\\App\\": "src/App/Service/",
          "App\\App\\Interfaces\\": "src/App/Service/Interfaces/",
          "App\\App\\Image\\": "src/App/Service/Image/",
          "App\\App\\Image\\Interfaces\\": "src/App/Service/Image/Interfaces/",
          "App\\App\\Security\\": "src/UI/Security/",
          "App\\App\\Security\\Interfaces\\": "src/UI/Security/Interfaces/",
          "App\\Domain\\Builder\\": "src/Domain/Builder/",
          "App\\Domain\\Builder\\Interfaces\\": "src/Domain/Builder/Interfaces/",
          "App\\Domain\\CollectionManager\\": "src/App/Service/CollectionManager/",
          "App\\Domain\\CollectionManager\\Interfaces\\": "src/App/Service/CollectionManager/Interfaces/",
          "App\\Domain\\DTO\\": "src/Domain/DTO/",
          "App\\Domain\\DTO\\Interfaces\\": "src/Domain/DTO/Interfaces/",
          "App\\Domain\\Entity\\": "src/Domain/Entity/",
          "App\\Domain\\Entity\\Interfaces\\": "src/Domain/Entity/Interfaces/",
          "App\\Domain\\Factory\\": "src/Domain/Factory/",
          "App\\Domain\\Factory\\Interfaces\\": "src/Domain/Factory/Interfaces/",
          "App\\Domain\\Repository\\": "src/Domain/Repository/",
          "App\\Domain\\Repository\\Interfaces\\": "src/Domain/Repository/Interfaces/",
          "App\\UI\\Action\\": "src/UI/Action/",
          "App\\UI\\Action\\Interfaces\\": "src/UI/Action/Interfaces/",
          "App\\UI\\Action\\Authentication\\": "src/UI/Action/Authentication/",
          "App\\UI\\Action\\Authentication\\Interfaces\\": "src/UI/Action/Interfaces/Authentication/",
          "App\\UI\\Form\\": "src/UI/Form/",
          "App\\UI\\Form\\Interfaces\\": "src/UI/Form/Interfaces/",
          "App\\UI\\Form\\Handler\\": "src/UI/Form/Handler/",
          "App\\UI\\Form\\Handler\\Interfaces\\": "src/UI/Form/Interfaces/Handler/",
          "App\\UI\\Form\\Subscriber\\": "src/UI/Form/Subscriber/",
          "App\\UI\\Form\\Subscriber\\Interfaces\\": "src/UI/Form/Interfaces/Subscriber/",
          "App\\UI\\Form\\Type\\": "src/UI/Form/Type/",
          "App\\UI\\Form\\Type\\Interfaces\\": "src/UI/Form/Interfaces/Type/",
          "App\\UI\\Form\\Type\\Authentication\\": "src/UI/Form/Interfaces/Type/Authentication/",
          "App\\UI\\Form\\Type\\Interfaces\\Authentication\\": "src/UI/Form/Interfaces/Type/Authentication/",
          "App\\UI\\Responder\\": "src/UI/Responder/",
          "App\\UI\\Responder\\Interfaces\\": "src/UI/Responder/Interfaces/"
      }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
